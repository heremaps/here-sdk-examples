<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#CCFFFFFF"/>
            <stroke android:color="#CC98A2AE"/>
        </shape>
    </item>
    <item android:state_activated="true" android:drawable="@color/level_background_selected"/>
    <item android:state_selected="true" android:drawable="@color/level_background_selected"/>
    <item android:state_selected="false" android:drawable="@color/level_background_normal"/>
</selector>
